using System;

class Program
{
    static void Main(string[] args)
    {
        string name1 = "Иосиф";
        string name2 = "Сафиров";
        char space = ' ';
        Console.WriteLine((name1) + (space) + (name2));
        Console.WriteLine("Ошибка!!!");
        (name1, name2) = (name2, name1);
        Console.WriteLine((name1) + (space) + (name2));
        Console.ReadLine();
    }
}
