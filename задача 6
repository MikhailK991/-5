using System;

class Program
{
    static void Main(string[] args)
    {
        int picture = 52;
        int picturesRow = 3;
        int numberOfLines = picture / picturesRow;
        int remainder = picture % picturesRow;
        Console.WriteLine($" {numberOfLines} рядов заполненых картинками \n Осталась {remainder} картинка");

    }
}
