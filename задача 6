using System;

class Program
{
    static void Main(string[] args)
    {
        int picture = 52;
        int picturesInRow = 3;
        int filledRows = picture / picturesInRow;
        int remainder = picture % picturesInRow;
        Console.WriteLine($" {filledRows} рядов заполненых картинками \n Осталась {remainder} картинка");

    }
}
