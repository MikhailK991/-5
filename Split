using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace MK
{
    class Program
    {
        static void Main(string[] args)
        {
            string text = "Век живи, век учись";
            var result = text.Split(' ');

            foreach (var word in result)
            { 
                Console.WriteLine(word + "\n");
            }
           
        }
    }
}
