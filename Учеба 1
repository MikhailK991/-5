using System;

class Program
{
    static void Main(string[] args)
    {
        Console.WriteLine("Привет. Что тебя порадует?");
        string userWishes = Console.ReadLine();
        Console.WriteLine("Сколько раз?");
        
        for (int numberOfRepetitions = Convert.ToInt32(Console.ReadLine()))
        {
            Console.WriteLine(userWishes);
        }
    }
}
